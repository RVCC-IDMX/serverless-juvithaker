{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-trailing-spaces": "error",
        "semi": "error",
        "linebreak-style": [
            "error",
            "unix"
        ]
    }
}